@model Shipper
@{
    ViewData["Title"] = "Shippers";
}
<h1>@ViewData["Title"]</h1>
<div>
    <div class="mb-3">
        <label for="shipperIdInput" class="form-label">Shipper Id</label>
        <input type="number" value="@Model.ShipperId" class="form-control" id="shipperIdInput" />
    </div>
    <div class="mb-3">
        <label for="companyNameInput" class="form-label">Company Name</label>
        <input value="@Model.CompanyName" class="form-control" id="companyNameInput"/>
    </div>
    <div class="mb-3">
        <label for="phoneInput" class="form-label">Phone</label>
        <input class="form-control" id="phoneInput" value="@Model.Phone" />
    </div>
</div>